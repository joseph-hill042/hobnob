{
  "name": "hobnob",
  "version": "0.1.0",
  "description": "The tutorial project for Enterprise JavaScript from Packt Publishing.",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/ && babel src -d dist",
    "serve": "npm run build && node dist/index.js",
    "test:e2e": "cucumber-js spec/cucumber/features --require-module @babel/register --require spec/cucumber/steps",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix": "eslint src/ --fix",
    "lint": "eslint src/",
    "watch": "nodemon -w src --exec npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/josephhill1/hobnob.git"
  },
  "keywords": [
    "JavaScript"
  ],
  "author": "Joseph Hill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/josephhill1/hobnob/issues"
  },
  "homepage": "https://github.com/josephhill1/hobnob#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "cucumber": "^5.0.2",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.18.6",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  }
}
